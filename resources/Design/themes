  <!-- SPA JS với cải tiến -->
  <script>
    const app = document.getElementById('app');
    const body = document.body;
    const sidebarOverlay = document.querySelector('.sidebar-overlay');
    const sidebarToggle = document.getElementById('sidebarToggle');
    
    // Kiểm tra trạng thái sidebar từ localStorage
    const isSidebarCollapsed = localStorage.getItem('sidebarCollapsed') === 'true';
    if (isSidebarCollapsed) {
      body.classList.add('sidebar-collapsed');
    }
    
    // Xử lý sự kiện toggle sidebar
    sidebarToggle.addEventListener('click', function(e) {
      e.preventDefault();
      body.classList.toggle('sidebar-collapsed');
      
      // Lưu trạng thái vào localStorage
      localStorage.setItem('sidebarCollapsed', body.classList.contains('sidebar-collapsed'));
      
      // Trên mobile, hiển thị/ẩn sidebar với overlay
      if (window.innerWidth < 768) {
        body.classList.toggle('mobile-sidebar-expanded');
      }
    });
    
    // Đóng sidebar khi click overlay trên mobile
    if (sidebarOverlay) {
      sidebarOverlay.addEventListener('click', function() {
        body.classList.remove('mobile-sidebar-expanded');
      });
    }

    // --- Tab modules (lazy load) ---
    const TabModules = 
    {
      employee_information: null, // Singleton sẽ tạo khi click lần đầu
      employee_hierarchy: null,
      employee_bank_information: null,

      hierarchy: null,
      payroll_rule: null,

      contract: null,
      salary_detail: null,

      attendance: null,
      
      leave: null,
    };

    function loadPage(page) {
      const content = app;
      
      // Thêm hiệu ứng fade-out trước khi tải nội dung mới
      content.style.opacity = 0;

      // Highlight active menu
      document.querySelectorAll('[data-page]').forEach(el => el.classList.remove('active'));
      const activeEl = document.querySelector(`[data-page="${page}"]`);
      if (activeEl) {
        activeEl.classList.add('active');
        
        // Mở collapse cha nếu có
        const parentCollapse = activeEl.closest('.collapse');
        if (parentCollapse) {
          const parentLink = document.querySelector(`[data-bs-target="#${parentCollapse.id}"]`);
          if (parentLink && parentLink.classList.contains('collapsed')) {
            new bootstrap.Collapse(parentCollapse, { show: true });
            parentLink.classList.remove('collapsed');
          }
        }
      }

      // Dùng setTimeout để đợi hiệu ứng fade-out hoàn tất rồi mới render và fade-in
      setTimeout(() => {
        // Load page content
        switch(page) {
          case 'dashboard': 
            content.innerHTML = `
              <h1 class="mt-4">Dashboard</h1>
              <ol class="breadcrumb mb-4"><li class="breadcrumb-item active">Dashboard</li></ol>
              <div class="row">
                <div class="col-xl-3 col-md-6">
                  <div class="card bg-primary text-white mb-4">
                    <div class="card-body">Primary Card</div>
                    <div class="card-footer d-flex align-items-center justify-content-between">
                      <a class="small text-white stretched-link" href="#">View Details</a>
                      <div class="small text-white"><i class="fas fa-angle-right"></i></div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-xl-6">
                  <div class="card mb-4">
                    <div class="card-header"><i class="fas fa-chart-area me-1"></i>Area Chart Example</div>
                    <div class="card-body"><canvas id="myAreaChart" width="100%" height="40"></canvas></div>
                  </div>
                </div>
                <div class="col-xl-6">
                  <div class="card mb-4">
                    <div class="card-header"><i class="fas fa-chart-bar me-1"></i>Bar Chart Example</div>
                    <div class="card-body"><canvas id="myBarChart" width="100%" height="40"></canvas></div>
                  </div>
                </div>
              </div>`;
            break;
          case 'employee_information': 
            if (typeof Employee_Information !== 'undefined') {
              Employee_Information.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Employee Information</h1><p>Module quản lý thông tin nhân viên</p>`;
            }
            break;
          case 'employee_hierarchy': 
            if (typeof Employee_Hierarchy !== 'undefined') {
              Employee_Hierarchy.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Employee Hierarchy</h1><p>Module quản lý phân cấp nhân viên</p>`;
            }
            break;
          case 'employee_bank_information': 
            if (typeof Employee_Bank_Information !== 'undefined') {
              Employee_Bank_Information.getInstance().render(content);
            } else {
              content.innerHTML = `<h1 class="mt-4">Employee Bank Information</h1><p>Module quản lý thông tin ngân hàng nhân viên</p>`;
            }
            break;
          case 'hierarchy': 
            if (typeof Hierarchy !== 'undefined') {
              Hierarchy.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Hierarchy</h1><p>Module quản lý phân cấp tổ chức</p>`;
            }
            break;
          case 'payroll_rule': 
            if (typeof Payroll_Rule !== 'undefined') {
              Payroll_Rule.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Payroll Rules</h1><p>Module quản lý quy tắc tính lương</p>`;
            }
            break;
          case 'contract': 
            if (typeof Contract !== 'undefined') {
              Contract.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Contracts</h1><p>Module quản lý hợp đồng</p>`;
            }
            break;
          case 'salary_detail': 
            if (typeof Salary_Detail !== 'undefined') {
              Salary_Detail.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Salary Details</h1><p>Module quản lý chi tiết lương</p>`;
            }
            break;
          case 'attendance': 
            if (typeof Attendance !== 'undefined') {
              Attendance.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Attendance</h1><p>Module quản lý chấm công</p>`;
            }
            break;
          case 'leave': 
            if (typeof Leave !== 'undefined') {
              Leave.getInstance().render(content); 
            } else {
              content.innerHTML = `<h1 class="mt-4">Leave Management</h1><p>Module quản lý nghỉ phép</p>`;
            }
            break;
          default: 
            content.innerHTML = `<p>Page not found: ${page}</p>`; 
            break;
        }

        // Kích hoạt hiệu ứng fade-in sau khi nội dung đã được render
        content.style.opacity = 1;
        window.location.hash = page;

        // Trên mobile, tự động đóng sidebar sau khi chọn mục menu
        if (window.innerWidth < 768) {
          body.classList.remove('mobile-sidebar-expanded');
        }

      }, 250); // 250ms = 0.25s, đủ để thấy hiệu ứng mờ đi
    }

    document.addEventListener('click', e => {
      const a = e.target.closest('[data-page]');
      if(!a) return;
      if(a.getAttribute('data-bs-toggle')) return;
      e.preventDefault();
      const page = a.getAttribute('data-page');
      if(page) loadPage(page);
    });

    // --- Load default page ---
    const defaultPage = window.location.hash ? window.location.hash.slice(1) : 'dashboard';
    loadPage(defaultPage);
  </script>
</body>
</html>